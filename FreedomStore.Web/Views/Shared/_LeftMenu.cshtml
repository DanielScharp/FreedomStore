@{
    Layout = null;
}
<button class="menu-toggle">
    <i class="fas fa-church"></i>
    <h3>Freedom Store</h3>
    <i class="fas fa-bars"></i>
</button>

<div class="sidebar">
    <div class="sidebar-header">
        <i class="fas fa-church"></i>
        <h3>Freedom Store</h3>
        <i class="fas fa-chevron-left toggle"></i>
        <button class="close-btn">
            <i class="fas fa-times"></i>
        </button>
    </div>

    <div class="sidebar-menu">
        <div class="menu-item active">
            <i class="fas fa-home"></i>
            <span>Início</span>
        </div>

        <div class="menu-item">
            <i class="fas fa-search"></i>
            <span>Busca</span>
        </div>

        <div class="menu-item with-submenu">
            <i class="fas fa-cog"></i>
            <span>Configurações</span>
            <i class="fas fa-chevron-down chevron"></i>
        </div>
        <div class="submenu">
            <div class="submenu-item">Perfil</div>
            <div class="submenu-item">Preferências</div>
            <div class="submenu-item">Privacidade</div>
        </div>

        <div class="menu-item">
            <i class="fas fa-history"></i>
            <span>Histórico</span>
        </div>

        <div class="menu-item with-submenu">
            <i class="fas fa-book"></i>
            <span>Documentação</span>
            <i class="fas fa-chevron-down chevron"></i>
        </div>
        <div class="submenu">
            <div class="submenu-item">API</div>
            <div class="submenu-item">Exemplos</div>
            <div class="submenu-item">Tutoriais</div>
        </div>

        <div class="menu-item">
            <i class="fas fa-question-circle"></i>
            <span>Ajuda</span>
        </div>
    </div>

    <div class="menu-footer">
        <button class="logout-btn">
            <i class="fas fa-sign-out-alt"></i>
            <span class="logout-text">Sair</span>
        </button>
    </div>
</div>
